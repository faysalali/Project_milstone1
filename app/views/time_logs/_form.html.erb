<div id="time_log-modal" class="modal fade">
  <div class="modal-dialog">
    <div class="modal-content">
      <%= form_for render_action(time_log, project), remote: true do |f| %>
        <%= render 'shared/error', object: time_log %>
        <div class= "model-body">
          <div class="form-group">
            <%= f.text_field :hours, placeholder: "Enter hours ", class: 'form-control' %>
            <%= f.submit class: 'btn btn-success' %>
           </div>
        </div>
      <% end %>
    </div>
  </div>
</div>
