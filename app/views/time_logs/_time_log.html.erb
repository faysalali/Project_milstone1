<tr id="time_log_<%= time_log.id %>">
  <td><%= time_log.user.name.capitalize %></td>
  <td><%= time_log.project.title.capitalize %></td>
  <td><%= time_log.hours %></td>
  <td>
    <%= link_to edit_time_log_path(time_log), remote: true do %><span class="glyphicon glyphicon-edit"></span><% end %>
    <%= link_to time_log_path(time_log), remote: true, method: :delete, data: { confirm: 'Are you sure?' } do %>
      <span class="glyphicon glyphicon-trash"></span>
    <% end %>
  </td>
</tr>
